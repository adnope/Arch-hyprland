{
  "layer": "top",
  "spacing": 0,
  "height": 0,
  "modules-left": [
    "hyprland/workspaces",
    "custom/right1",
    "hyprland/window"
  ],
  "modules-center": [
    "custom/left1",
    "temperature",
    "custom/left2",
    "memory",
    "custom/left3",
    "cpu",
    "custom/leftin1",
    "custom/left_distro",
    "custom/distro",
    "custom/right_distro",
    "custom/rightin1",
    "clock#time",
    "custom/right2",
    "clock#date",
    "custom/right3",
    "custom/weather",
    "custom/right4"
  ],
  "modules-right": [
    "custom/left4",
    "tray",
    "custom/left5",
    "pulseaudio",
    "custom/left6",
    "backlight",
    "custom/left7",
    "battery",
    "custom/leftin2",
    "custom/network"
  ],
  "hyprland/workspaces": {
    "disable-scroll": true,
    "all-outputs": true,
    "tooltip": false
  },
  "hyprland/window": {
    "format": " {title}",
    "max-length": 25,
    "rewrite": {
      // "(.*) Zen Browser": "üåé$1"
    }
  },
  "temperature": {
    "format": "Û∞îè {}¬∞C",
    "interval": 5,
    "min-length": 6,
    "max-length": 6
  },
  "memory": {
    "states": {
      "warning": 75,
      "critical": 90
    },
    "format": "Û∞òö {percentage}%",
    "format-critical": "Û∞Ä¶ {percentage}%",
    "tooltip": true,
    "tooltip-format": "Used: {used:0.1f} GB / {total:0.1f} GB",
    "interval": 5,
    "min-length": 6,
    "max-length": 6
  },
  "cpu": {
    "format": "Û∞ª† {usage}%",
    "tooltip": true,
    "interval": 5,
    "min-length": 6,
    "max-length": 6
  },
  "custom/distro": {
    "format": " ÔåÉ ",
    "tooltip": false
  },
  "clock#time": {
    "format": "Ôê∫ {:%H:%M}",
    "tooltip": false,
    // "tooltip-format": "Standard Time: {:%I:%M %p}",
    "min-length": 8,
    "max-length": 8
  },
  "clock#date": {
    "format": "Û∞®≥ {:%d-%m}",
    "tooltip-format": "<tt>{calendar}</tt>",
    "calendar": {
      "mode": "month",
      "mode-mon-col": 6,
      "on-click-right": "mode",
      "format": {
        "months": "<span color='#b4befe'><b>{}</b></span>",
        "weekdays": "<span color='#a6adc8' font='7'>{}</span>",
        "today": "<span color='#f38ba8'><b>{}</b></span>"
      }
    },
    "min-length": 8,
    "max-length": 8
  },
  "custom/network": {
    "exec": "$HOME/.config/waybar/scripts/networkspeed.sh",
    "interval": 1,
    "min-length": 16
  },
  "tray": {
    "spacing": 12,
    "tooltip": false
  },
  "pulseaudio": {
    "format": "{icon} {volume}%",
    "format-muted": "Û∞ùü {volume}%",
    "format-icons": {
      "default": [
        "Û∞ïø",
        "Û∞ñÄ",
        "Û∞ïæ"
      ],
      "headphone": "Û∞ãã",
      "headset": "Û∞ãã"
    },
    "tooltip-format": "Device: {desc}",
    "on-click": "wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle",
    "on-click-right": "pavucontrol",
    "on-scroll-up": "wpctl set-volume -l 1 @DEFAULT_AUDIO_SINK@ 5%+",
    "on-scroll-down": "wpctl set-volume -l 1 @DEFAULT_AUDIO_SINK@ 5%-"
  },
  "backlight": {
    "format": "{icon} {percent}%",
    "format-icons": [
      "Óéç",
      "Óèì",
      "Óèë",
      "Óèè",
      "Óèé",
      "Óèç",
      "Óèä",
      "Óèà",
      "Óéõ"
    ],
    "tooltip": false,
    "on-scroll-up": "brightnessctl set 5%+",
    "on-scroll-down": "brightnessctl set 5%-"
  },
  "battery": {
    "states": {
      "warning": 20,
      "critical": 10
    },
    "format": "{icon} {capacity}%",
    "format-icons": [
      "Û∞Çé",
      "Û∞Åª",
      "Û∞Åº",
      "Û∞ÅΩ",
      "Û∞Åæ",
      "Û∞Åø",
      "Û∞ÇÄ",
      "Û∞ÇÅ",
      "Û∞ÇÇ",
      "Û∞Åπ"
    ],
    "format-charging": "ÔÉß {capacity}%",
    "tooltip-format": "Discharging: {time}",
    "tooltip-format-charging": "Charging: {time}",
    "interval": 1
  },
  "custom/weather": {
    "exec": "$HOME/.config/waybar/scripts/weather.sh",
    "return-type": "string",
    "interval": 300
  },
